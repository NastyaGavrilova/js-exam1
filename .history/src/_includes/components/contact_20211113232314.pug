mixin contact()
  .c-contact 
    .c-contact__wrapper 
      .c-contact__title 
        +title({
          classList:"e-title--montserrat",
          content:"OUR LOCATION",
        })
      .c-contact__map 
        #js-map
      .c-contact__content 
        .c-contact__subtitle 
          h3.e-contact__title 
